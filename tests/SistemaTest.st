'From Pharo8.0.0 of 22 January 2020 [Build information: Pharo-8.0.0+build.1124.sha.0932da82f08175e906b0e2a8052120c823374e9f (64 Bit)] on 17 April 2020 at 6:33:37.834051 pm'!TestCase subclass: #SistemaTest	instanceVariableNames: 'sistema u1 p1 t1'	classVariableNames: ''	package: 'Quoora-tests'!!SistemaTest methodsFor: 'setup' stamp: 'nazamoresco 3/28/2020 21:49'!setUp		sistema := Sistema new.		t1 := Topico nombre:'Informatica' descripcio:'Todos los temas relacionados con el mundo de computacion.'.	u1 := Usuario nombre:'Jorge' contraseña:'123contrasena'.	p1 := Pregunta texto: 'Que es dios?' autor: u1 titulo: 'Ayuda'.		! !!SistemaTest methodsFor: 'tests' stamp: 'nazamoresco 4/17/2020 18:17'!testBuscarUsuariosContraseña		sistema agregarUsuario: u1. 		self assert: (sistema buscarUsuarios: 'Jorge' contraseña: '123contrasena') equals: u1.! !!SistemaTest methodsFor: 'tests' stamp: 'nazamoresco 4/17/2020 18:26'!testDevolverPreguntasRelevantesEnOrden|p2|	sistema agregarPregunta: p1.	p2 := Pregunta texto: 'No entiendo' autor: u1 titulo: 'Ayuda'.	sistema agregarPregunta: p2.	u1 agregarTopico: t1.	p1 agregarTopico: t1.	p2 agregarTopico: t1.	self assert: (((sistema devolverPreguntasRelevantesEnOrden: u1) first) = p2)! !!SistemaTest methodsFor: 'tests' stamp: 'nazamoresco 4/17/2020 18:28'!testDevolverTopicoPorNombre	sistema agregarTopico: t1.	self assert: (sistema devolverTopicoPorNombre: 'Informatica') = t1.! !!SistemaTest methodsFor: 'as yet unclassified'!testObtenerPreguntasTopico		sistema agregarPregunta: p1.		p1 agregarTopico: t1.		self assert:((sistema obtenerPreguntasTopico: t1) includes:p1)  ! !!SistemaTest methodsFor: 'as yet unclassified'!testDevolverPuntaje	sistema agregarPregunta: p1.	self assert:(( sistema devolverPuntaje: u1 )= 20)! !!SistemaTest methodsFor: 'as yet unclassified' stamp: 'nazamoresco 4/17/2020 18:24'!testAgregarPregunta	sistema agregarPregunta: p1.			self assert: (sistema preguntas includes: p1)! !!SistemaTest methodsFor: 'as yet unclassified'!testPreguntasDeUsuario	sistema agregarPregunta:p1.	self assert: ((sistema preguntasDeUsuario: u1) includes: p1)! !!SistemaTest methodsFor: 'as yet unclassified' stamp: 'nazamoresco 3/28/2020 22:10'!testAgregarTopico	sistema agregarTopico: t1.	self assert: (sistema topicos includes: t1)! !!SistemaTest methodsFor: 'as yet unclassified'!testBorrarPregunta	sistema agregarPregunta: p1.	sistema borrarPregunta: p1. 	self deny: (sistema preguntas includes: p1 )! !!SistemaTest methodsFor: 'as yet unclassified'!testRespuestasDeUsuario	|r1|	sistema agregarPregunta: p1.	r1 := Respuesta texto:'Dios no es, lo somos.'  autor:u1  pregunta:p1.	self assert:( (sistema respuestasDeUsuario: u1 ) includes: r1)	 ! !!SistemaTest methodsFor: 'as yet unclassified' stamp: 'nazamoresco 3/28/2020 22:10'!testAgregarUsuario	sistema agregarUsuario: u1. 	self assert: (sistema usuarios includes: u1)! !!SistemaTest methodsFor: 'as yet unclassified'!testDevolverPreguntasRelevantes	sistema agregarPregunta: p1.	u1 agregarTopico: t1.	p1 agregarTopico: t1.	self assert: ((sistema devolverPreguntasRelevantes: u1) includes: p1)! !