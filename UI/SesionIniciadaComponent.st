 'as yet unclassified' stamp: 'franBeb 4/16/2020 16:24'!logOut	self session logueado: nil.	self call: InicioComponent new.	! !!SesionIniciadaComponent methodsFor: 'as yet unclassified' stamp: 'franBeb 4/17/2020 13:00'!mostrarPregunta: preg en: aCanvas	aCanvas anchor							callback: [ self verPregunta: preg];							with: [ aCanvas paragraph: preg titulo].	aCanvas text: 'Topicos: '.		(preg topicos) do:[:topico| aCanvas text: topico nombre. aCanvas text: ', '.].	aCanvas break.	self realizadoPor: preg autor en: aCanvas.	aCanvas text: ' | ';text: preg respuestas size;text:' respuestas.';break.	self likesyDislikes: aCanvas  dePost: preg.	aCanvas break.! !!SesionIniciadaComponent methodsFor: 'as yet unclassified' stamp: 'franBeb 4/17/2020 12:00'!regresarself answer.! !!SesionIniciadaComponent methodsFor: 'accessing' stamp: 'nazamoresco 4/6/2020 19:16'!sist	^ sist! !!SesionIniciadaComponent methodsFor: 'initialization' stamp: 'nazamoresco 4/6/2020 19:15'!initialize super initialize.sist:= Sistema soleInstance .! !