'From Pharo8.0.0 of 22 January 2020 [Build information: Pharo-8.0.0+build.1124.sha.0932da82f08175e906b0e2a8052120c823374e9f (64 Bit)] on 11 April 2020 at 5:32:27.696798 pm'!WAComponent subclass: #SesionIniciadaComponent	instanceVariableNames: 'sist'	classVariableNames: ''	package: 'Quoora-UI'!!SesionIniciadaComponent methodsFor: 'as yet unclassified' stamp: 'franBeb 4/10/2020 19:23'!verPregunta: preg	| pregComp|	pregComp := PreguntaComponent new: preg.	self call: pregComp.! !!SesionIniciadaComponent methodsFor: 'as yet unclassified' stamp: 'franBeb 4/10/2020 20:35'!verAutor: autor	| auComp|	auComp := AutorComponent new: autor.	self call: auComp.! !!SesionIniciadaComponent methodsFor: 'as yet unclassified' stamp: 'franBeb 4/10/2020 20:11'!volver: aCanvasaCanvas  anchor 				callback: [ self answer];				with: [ aCanvas paragraph: 'volver' ].! !!SesionIniciadaComponent methodsFor: 'as yet unclassified' stamp: 'franBeb 4/10/2020 19:59'!logOut	self session logueado: nil.	self answer .	! !!SesionIniciadaComponent methodsFor: 'as yet unclassified' stamp: 'franBeb 4/10/2020 20:01'!mostrarPregunta: preg en: aCanvas	aCanvas anchor							callback: [ self verPregunta: preg];							with: [ aCanvas paragraph: preg titulo].							(preg topicos) do:[:topico| aCanvas paragraph: topico nombre].														aCanvas anchor							callback: [ self verAutor:preg autor];							with: [ aCanvas paragraph: preg autor nombre.].																					aCanvas paragraph: preg respuestas size.							aCanvas anchor								callback: [ preg votar: true deUsuario: self session logueado ];								with: [ aCanvas paragraph: preg likes size ].							aCanvas anchor								callback: [ preg votar: false deUsuario: self session logueado ];								with: [ aCanvas paragraph: preg disLikes size ] ! !!SesionIniciadaComponent methodsFor: 'initialization' stamp: 'nazamoresco 4/6/2020 19:15'!initialize super initialize.sist:= Sistema soleInstance .! !!SesionIniciadaComponent methodsFor: 'rendering' stamp: 'nazamoresco 4/6/2020 19:17'!renderSesion: aCanvasaCanvas label: self session logueado nombre.			aCanvas anchor				callback: [ self logOut ];				with: [ aCanvas button: 'LogOut' ].! !!SesionIniciadaComponent methodsFor: 'accessing' stamp: 'nazamoresco 4/6/2020 19:16'!sist	^ sist! !